    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "env": {
                "postgres_users_host":      "localhost",
                "postgres_users_port":      5432,
                "postgres_users_user":      "postgres",
                "postgres_users_password":  "admin",
                "postgres_users_dbname":    "users"
            },
            "args": []
        },
        {
            "name": "Attach to local process",
            "type": "go",
            "request": "attach",
            "mode": "local",
            "processId": 0
        },
        {
            "name": "Launch test package",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${workspaceFolder}"
        }
    ]
}